{
    "name": "starter",
    "description": "A clean, minimal default theme for the Ghost publishing platform",
    "demo": "https://starter.webthemes.dev",
    "version": "1.0.0",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "license": "GPL",
    "author": {
        "name": "Web Themes",
        "email": "info@webthemes.io",
        "url": "https://webthemes.io"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Magazine"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/webthemes/starter.git"
    },
    "bugs": "https://github.com/webthemes/starter/issues",
    "contributors": "https://github.com/webthemes/starter/graphs/contributors",
    "browserslist": [
        "defaults"
    ],
    "config": {
        "posts_per_page": 25,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    }
}
